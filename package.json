{
  "name": "romeo.html",
  "version": "0.0.1",
  "description": "Lightweight Ledger built on top of IOTA",
  "main": "index.js",
  "author": "Roman Semko",
  "license": "ISC",
  "scripts": {
    "fix": "prettier --single-quote --write src/**/*.{js,css} package.json *.md",
    "clean": "rimraf dist",
    "dist": "parcel build src/index.html --public-url \"/\"",
    "pack": "html-inline -i dist/index.html -o romeo-`cat package.json | jq -r '.version'`.html -b dist",
    "build": "yarn run clean && yarn run dist && yarn run pack",
    "run": "parcel src/index.html"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "html-inline": "^1.2.0",
    "parcel-bundler": "^1.5.1",
    "prettier": "1.10.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "better-queue": "^3.8.6",
    "copy-to-clipboard": "^3.0.8",
    "history": "^4.7.2",
    "pretty-ms": "^3.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-toastify": "^3.4.3",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.79.0"
  }
}
